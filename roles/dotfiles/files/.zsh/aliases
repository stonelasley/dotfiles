#
# Command aliases
#
alias ....='cd ../..'
alias cd..='cd ..'
alias :e=vim
alias :q=exit
alias :sp='test -n $TMUX && tmux split-window'
alias :vs='test -n $TMUX && tmux split-window -h'
alias t="tmux"
alias v=vim
alias vrc="vim $HOME/.vimrc"
alias zrc="vim $HOME/.zshrc"
alias trc="vim $HOME/.tmux.conf"
alias st1="cd ~/.st1"
alias docker=docker.exe
alias docker-compose=docker-compose.exe

#
# Random Projects
#
PROJECTHOME="/mnt/p/Projects" 
alias projects="cd $PROJECTHOME"
alias blendtec="cd $PROJECTHOME/Blendtec"
alias freshbyte="cd $PROJECTHOME/Freshbyte"
alias personal="cd $PROJECTHOME/Personal"
alias uinta="cd $PROJECTHOME/Uinta"
alias scratch="cd $PROJECTHOME/Scratch"
alias edible="cd $PROJECTHOME/Edible"
